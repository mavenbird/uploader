<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mavenbird_imageuploader>
            <extensions>
                <vector>
                    <svg>svg</svg>
                    <svg_xml>svg+xml</svg_xml>
                </vector>
                <web_image>
                    <webp>webp</webp>
                </web_image>
            </extensions>
        </mavenbird_imageuploader>
        <dev>
            <image>
                <adapters>
                    <GD2>
                        <class>Mavenbird\ImageUploader\Model\Image\Adapter\Gd2</class>
                    </GD2>
                </adapters>
            </image>
        </dev>
    </default>
</config>
